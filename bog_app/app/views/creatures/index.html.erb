<nav class="navbar navbar-default navbar-fixed-top">
  <div class="container">
    <ul class="nav navbar-nav">
      <li><%= link_to "Home", creatures_path %></li>
      <li><%= link_to "Create New Creature", new_creature_path %></li>
    </ul>
  </div>
</nav>
<div class="container-fluid">
  <h1>Creature Index page</h1>

  <% @creatures.each do |creature| %>
    <ul>
      <li>
        <strong>Id:</strong> <%= creature.id %><br>
        <strong>Name:</strong> <%= creature.name %><br>
        <strong>Description:</strong> <%= creature.description %><br>
        <%= link_to "Show Creature", creature_path(creature) %>
      </li>
    </ul>
  <% end %>
</div>
